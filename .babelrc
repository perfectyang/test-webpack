{
  "presets": [
    [
        "@babel/preset-env",
        {
            "modules": false,
            "targets": {
                "browsers": [
                    "> 1%",
                    "iOS >= 7",
                    "Android >= 4.0",
                    "not ie <= 8",
                    "Firefox > 20"
                ]
            },
            "useBuiltIns": "usage",
            "corejs": 2
        }

    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": 2
    }],
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }]
  ]
}
